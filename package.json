{
  "name": "@azure/cosmos-query-editor-react",
  "version": "1.0.0-beta.2",
  "description": "Query editor for Azure Cosmos DB frontends",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write \"{src,test}/**/*.{ts,tsx,html}\"",
    "format:check": "prettier --check \"{src,test}/**/*.{ts,tsx,html}\"",
    "pretest": "tsc --noEmit && npm run lint && npm run format:check",
    "build": "npm run pretest && npm run clean && npx parcel build && api-extractor run --local && cp package.json README.md src/QueryEditor.css ./dist",
    "extract-api": "tsc -p . && api-extractor run --local",
    "publish": "npm run build && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/azure-cosmos-db-query-editor.git"
  },
  "keywords": [
    "azure",
    "query",
    "editor",
    "cosmosdb",
    "react",
    "reactjs"
  ],
  "author": "Azure Cosmos DB",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/azure-cosmos-db-query-editor/issues"
  },
  "homepage": "https://github.com/Azure/azure-cosmos-db-query-editor#readme",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@fluentui/react": "8.104.0",
    "json-pointer": "0.6.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.34.4",
    "@parcel/packager-ts": "^2.8.0",
    "@parcel/transformer-typescript-tsc": "^2.8.0",
    "@parcel/transformer-typescript-types": "^2.8.0",
    "@parcel/transformer-css": "2.8.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.4",
    "@types/json-pointer": "1.0.31",
    "@types/react": "18.0.25",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "8.29.0",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^29.5.0",
    "jest-css-modules-transform": "4.4.2",
    "jest-environment-jsdom": "29.5.0",
    "parcel": "^2.8.0",
    "prettier": "2.8.0",
    "ts-jest": "^29.0.5",
    "typescript": "4.9.4"
  }
}
